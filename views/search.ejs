<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">

<!--   <header class="main-header">
    <a href="#" class="logo">
      <span class="logo-lg">Productos</span>
    </a>
  </header> -->
  
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <!-- Main content -->
    <section class="content">
      <div class="row">
        <!-- left column -->
        <div class="col-md-6">
          <!-- general form elements -->

          <% producList.forEach(function(item) { %>
		  <div class="box box-primary">           
            <form role="form">
              <div class="box-body">
                <div class="form-group">
                    <img className='product-image' src="<%= item.images %>"/>
                    <h3><%= item.name %></h3>
                    <p className='static-page-subtitle'><%= item.name %></p>
                </div>
                
                <Button class="btn btn-primary">Ver</Button>
              </div>
            </form>
          </div>
          <% }); %> 
        </div>
        <!--/.col (right) -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
  <footer class="main-footer">
    <strong>Copyright &copy; 2018 <a href="http://cotodigital3.com.ar">COTO</a>.</strong> All rights
    reserved.
  </footer>  
</div>
